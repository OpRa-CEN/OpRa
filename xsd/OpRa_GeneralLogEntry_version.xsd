	<!-- ======================================================================= -->
	<!-- == NeTEX Entities for holding differences between versions of an ENTITY In VERSION=============================================================== -->
	<xsd:complexType name="LoggableObjects_RelStructure">
		<xsd:annotation>
			<xsd:documentation>A collection of one or more TRACEs.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="LoggableObject" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A way to record the context of the changes occurred in a given ENTITY instance, as regards the authors, the causes of the changes, etc., possibly accompanied by a descriptive text.he next one.  A TRACE contains pairs of attributes' old values - new values.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="LoggableObject" type="LoggableObjectStructure">
		<xsd:annotation>
			<xsd:documentation>An entity for which LOG ENTRies may be made.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="LoggableObjectStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a TYPE OF TRACE.</xsd:documentation>
		</xsd:annotation>
		<xsd:group ref="LoggableObjectGroup">
			<xsd:annotation>
				<xsd:documentation>Elements for a LOGGABLE OBJECT.</xsd:documentation>
			</xsd:annotation>
		</xsd:group>
		<xsd:attribute name="id" type="LoggableObjectIdType">
			<xsd:annotation>
				<xsd:documentation>Identifier of LOGGABLE OBJECT</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="created" type="xsd:dateTime" use="optional">
			<xsd:annotation>
				<xsd:documentation>Date at which the LOGGABLE OBJECT was created.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:group name="LoggableObjectGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a TYPE OF TRACE.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Description" type="xsd:normalizedString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Description of LOGGABLE OBJECT</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LogEntries" type="GeneralLogEntries_RelStructure"/>
		</xsd:sequence>
	</xsd:group>
	<!-- ======================================================================= -->
	<xsd:complexType name="GeneralLogEntries_RelStructure">
		<xsd:annotation>
			<xsd:documentation>A generic LOGENTRY.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="GeneralLogEntry" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A time-stamped record of an event or change of state.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="GeneralLogEntry" type="GeneralLogEntryStructure">
		<xsd:annotation>
			<xsd:documentation>A  record of the detailed changes of a given ENTITY IN VERSION from one VERSION to the next one.  A DELTA contains pairs of attributes' old values - new values.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="GeneralLogEntryStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a DELTA.</xsd:documentation>
		</xsd:annotation>
		<xsd:group ref="GeneralLogEntryGroup">
			<xsd:annotation>
				<xsd:documentation>Elements for a LOG ENTRY</xsd:documentation>
			</xsd:annotation>
		</xsd:group>
		<xsd:attribute name="id" type="LogEntryIdType">
			<xsd:annotation>
				<xsd:documentation>Identifier of LOG ENTRY</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="created" type="xsd:dateTime" use="optional">
			<xsd:annotation>
				<xsd:documentation>Date reference was first created.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:group name="GeneralLogEntryGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a DELTA.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="SimpleObjectRef"/>
			<xsd:element name="FromVersionRef" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Base version against which  LOG ENTRY is made.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:restriction base="SimpleObjectRefStructure">
							<xsd:attribute name="ref" type="VersionIdType" use="required">
								<xsd:annotation>
									<xsd:documentation>Identifier of referenced entity.</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
						</xsd:restriction>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Description" type="xsd:normalizedString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Description of LOG ENTRY</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Name" type="MultilingualString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Name of the LOG ENTRY</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoggedAtTime" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>Date at which the LOG ENTRY was captured</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LogEntryValues" type="LogEntryValues_RelStructure">
				<xsd:annotation>
					<xsd:documentation>Values picked up from the ENTITY as input for the LOGGABLE OBJECT.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ======================================================================= -->
	<xsd:complexType name="LogEntryValues_RelStructure">
		<xsd:annotation>
			<xsd:documentation>A collection of one or more DELTA VALUEs.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="LogEntryValue" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Record of the values to be captured by a LOG ENTRY</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="LogEntryValue" type="LogEntryValueStructure">
		<xsd:annotation>
			<xsd:documentation>A  record of the detailed changes of a given ENTITY IN VERSION from one VERSION to the next one.  A DELTA contains pairs of attributes' old values - new values.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="LogEntryValueStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a DELTA VALUE.</xsd:documentation>
		</xsd:annotation>
		<xsd:group ref="LogEntryValueGroup">
			<xsd:annotation>
				<xsd:documentation>Elements for a DELTA VALUE.</xsd:documentation>
			</xsd:annotation>
		</xsd:group>
		<xsd:attribute name="id" type="LogEntryValueIdType">
			<xsd:annotation>
				<xsd:documentation>Identifier of DELTA.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:group name="LogEntryValueGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a DELTA VALUE.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="LogEntryRef" type="LogEntryIdType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifier of LOG ENTRY of which this is part.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ValueName" type="xsd:normalizedString">
				<xsd:annotation>
					<xsd:documentation>Name of VALUE captured by the LOG ENTRY.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Value" type="xsd:anyType">
				<xsd:annotation>
					<xsd:documentation>VALUE captured by the LOG ENTRY.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
</xsd:schema>