    <!-- ======================================================================= -->
    <xsd:group name="SimpleEventRefGroup">
        <xsd:annotation>
            <xsd:documentation>Context definition, indicator value and unit of measure for a key performance INDICATOR.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="EventType" type="EventTypeEnumeration">
                <xsd:annotation>
                    <xsd:documentation>Type of EVENT.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EventId" type="EventIdType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>ID of the EVENT in a linked system or module.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EventSummary" type="xsd:normalizedString" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Summary placeholder for the EVENT.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:group>
    <xsd:complexType name="SimpleEventRefs_RelStructure">
        <xsd:annotation>
            <xsd:documentation>A collection of one or more simplified EVENT references.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="SimpleEventRef" type="SimpleEventRefStructure" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A simplified EVENT reference within the collection.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
