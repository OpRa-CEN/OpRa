	<!-- ======================================================================= -->
	<xsd:simpleType name="DeckPlanIdType">
		<xsd:restriction base="netex:ObjectIdType"/>
	</xsd:simpleType>
	<xsd:complexType name="DeckPlanRefStructure">
		<xsd:simpleContent>
			<xsd:restriction base="netex:VersionOfObjectRefStructure">
				<xsd:attribute name="ref" type="DeckPlanIdType" use="required"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:element name="DeckPlan">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="DeckPlan_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="netex:EntityInVersionGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="netex:DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:group ref="DeckPlanGroup"/>
					</xsd:sequence>
					<xsd:attribute name="id" type="DeckPlanIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="DeckPlan_VersionStructure">
		<xsd:complexContent>
			<xsd:extension base="netex:DataManagedObjectStructure">
				<xsd:group ref="DeckPlanGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="DeckPlanGroup">
		<xsd:sequence>
			<xsd:element name="decks" type="Decks_RelStructure"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="DeckPlans_RelStructure">
		<xsd:sequence>
			<xsd:element ref="DeckPlan" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="DeckIdType">
		<xsd:restriction base="netex:ObjectIdType"/>
	</xsd:simpleType>
	<xsd:complexType name="DeckRefStructure">
		<xsd:simpleContent>
			<xsd:restriction base="netex:VersionOfObjectRefStructure">
				<xsd:attribute name="ref" type="DeckIdType" use="required"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:element name="Deck">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="Deck_VersionStructure">
					<xsd:sequence>
						<xsd:group ref="netex:EntityInVersionGroup"/>
						<xsd:group ref="netex:DataManagedObjectGroup"/>
					</xsd:sequence>
					<xsd:attribute name="id" type="DeckIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="Deck_VersionStructure">
		<xsd:complexContent>
			<xsd:extension base="netex:DataManagedObjectStructure">
				<xsd:group ref="DeckGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="DeckGroup">
		<xsd:sequence>
			<xsd:element name="deckSpaces" minOccurs="0"/>
			<xsd:element name="deckEntrances" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="Decks_RelStructure">
		<xsd:sequence>
			<xsd:element ref="Deck" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
