        <xsd:documentation>OpRa PLANNED SERVICE FRAME.</xsd:documentation>
    </xsd:annotation>
    <!-- ======================================================================= -->
    <xsd:simpleType name="PlannedServiceFrameIdType">
        <xsd:annotation>
            <xsd:documentation>Type for identifier of a OpRa FRAME.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="netex:VersionFrameIdType" />
    </xsd:simpleType>
    <xsd:element name="PlannedServiceFrameRef" type="PlannedServiceFrameRefStructure" substitutionGroup="netex:VersionFrameRef" abstract="false">
        <xsd:annotation>
            <xsd:documentation>Reference to an OpRa FRAME.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType abstract="false" name="PlannedServiceFrameRefStructure">
        <xsd:annotation>
            <xsd:documentation>Type for a reference to a OpRa FRAME.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="netex:VersionFrameRefStructure">
                <xsd:attribute name="ref" type="PlannedServiceFrameIdType" use="required" />
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
    <!-- ======================================================================= -->
    <xsd:element name="PlannedServiceFrame" substitutionGroup="netex:CommonFrame" abstract="false">
        <xsd:annotation>
            <xsd:documentation>A coherent set of OpRa data to which the same frame VALIDITY CONDITIONs have been assigned.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:restriction base="OpRa_PlannedService_VersionFrameStructure">
                    <xsd:sequence>
                        <xsd:sequence>
                            <xsd:group minOccurs="0" ref="netex:EntityInVersionGroup" />
                        </xsd:sequence>
                        <xsd:sequence>
                            <xsd:group ref="netex:DataManagedObjectGroup" />
                        </xsd:sequence>
                        <xsd:sequence>
                            <xsd:group ref="netex:VersionFrameGroup" />
                        </xsd:sequence>
                        <xsd:sequence>
                            <xsd:group ref="PlannedServiceFrameGroup" />
                        </xsd:sequence>
                    </xsd:sequence>
                    <xsd:attribute name="id" type="PlannedServiceFrameIdType" />
                </xsd:restriction>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>
    <xsd:complexType abstract="false" name="OpRa_PlannedService_VersionFrameStructure">
        <xsd:annotation>
            <xsd:documentation>Type for a OpRa FRAME.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="netex:Common_VersionFrameStructure">
                <xsd:sequence>
                    <xsd:group ref="PlannedServiceFrameGroup" />
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:group name="PlannedServiceFrameGroup">
        <xsd:annotation>
            <xsd:documentation>Elements for an OpRa PLANNED SERVICE FRAME.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="NetexUrl" type="xsd:anyURI" minOccurs="0" />
            <xsd:group ref="PlannedServiceInFrameGroup" />
        </xsd:sequence>
    </xsd:group>
    <!-- ======================================================================= -->
    <xsd:group name="PlannedServiceInFrameGroup">
        <xsd:annotation>
            <xsd:documentation>OpRa related elements in frame.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="NetexPayload" type="netex:dataObjects_RelStructure" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>General LOG ENTRies in frame.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:group>
    <!-- ======================================================================= -->
</xsd:schema>