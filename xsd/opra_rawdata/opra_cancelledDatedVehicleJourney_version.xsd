	<xsd:include schemaLocation="../opra_framework/opra_genericFramework/opra_GeneralLogEntry_version.xsd"/>
	<xsd:include schemaLocation="../opra_framework/opra_reusableComponents/opra_ptScope_support.xsd"/>
	<xsd:include schemaLocation="../opra_framework/opra_reusableComponents/opra_durationDistribution_support.xsd"/>
	<xsd:include schemaLocation="../opra_framework/opra_reusableComponents/opra_statistics_support.xsd"/>
	<xsd:annotation>
