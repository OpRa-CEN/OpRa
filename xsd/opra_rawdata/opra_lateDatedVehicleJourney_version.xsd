    <xsd:simpleType name="LateDatedVehicleJourneyCountIdType">
        <xsd:annotation>
            <xsd:documentation>Identifier type for LATE DATED VEHICLE JOURNEY.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="LoggableObjectIdType" />
    </xsd:simpleType>
    <xsd:complexType name="LateDatedVehicleJourneyCountRefStructure">
        <xsd:annotation>
            <xsd:documentation>Reference to a LATE DATED VEHICLE JOURNEY.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="LoggableObjectRefStructure">
                <xsd:attribute name="ref" type="LateDatedVehicleJourneyCountIdType" use="required" />
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
    <!-- ======================================================================= -->
    <xsd:element name="LateDatedVehicleJourneyCount">
        <xsd:annotation>
            <xsd:documentation>Implementation of LOGGABLE OBJECT designed for recording LATE DATED VEHICLE JOURNEY information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:restriction base="LateDatedVehicleJourneyCount_VersionStructure">
                    <xsd:sequence>
                        <xsd:sequence>
                            <xsd:group minOccurs="0" ref="netex:EntityInVersionGroup" />
                        </xsd:sequence>
                        <xsd:sequence>
                            <xsd:group ref="netex:DataManagedObjectGroup" />
                        </xsd:sequence>
                        <xsd:sequence>
                            <xsd:group ref="LateDatedVehicleJourneyCountGroup" />
                        </xsd:sequence>
                    </xsd:sequence>
                    <xsd:attribute name="id" type="LateDatedVehicleJourneyCountIdType" />
                </xsd:restriction>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>
    <xsd:complexType name="LateDatedVehicleJourneyCount_VersionStructure">
        <xsd:annotation>
            <xsd:documentation>Type for a LOGGABLE OBJECT designed for recording LATE DATED VEHICLE JOURNEY information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="LoggableObject_VersionStructure">
                <xsd:sequence>
                    <xsd:group ref="LateDatedVehicleJourneyCountGroup" />
                    <xsd:group ref="DurationDistributionGroup" />
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:group name="LateDatedVehicleJourneyCountGroup">
        <xsd:annotation>
            <xsd:documentation>Elements for the LATE DATED VEHICLE JOURNEY.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:group ref="PtScopeGroup" />
            <xsd:element name="entries" type="LateDatedVehicleJourneyEntries_RelStructure" minOccurs="0" />
        </xsd:sequence>
    </xsd:group>
    <xsd:complexType name="LateDatedVehicleJourneyCounts_RelStructure">
        <xsd:annotation>
            <xsd:documentation>A collection of one or more LateDatedVehicleJourneys.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element ref="LateDatedVehicleJourneyCount" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>LOGGABLE OBJECT designed for recording DATED VEHICLE JOURNEY information.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="LateDatedVehicleJourneyEntry" type="LateDatedVehicleJourneyEntry_Structure" />
    <xsd:complexType name="LateDatedVehicleJourneyEntry_Structure">
        <xsd:complexContent>
            <xsd:extension base="netex:LogEntry_VersionStructure">
                <xsd:group ref="LateDatedVehicleJourneyEntryGroup" />
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:group name="LateDatedVehicleJourneyEntryGroup">
        <xsd:annotation>
            <xsd:documentation>Elements for a LOG ENTRY related to a LateDatedVehicleJourneyEntry.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:group ref="PtScopeGroup" />
            <xsd:element name="SituationRef" type="siri:SituationSimpleRefStructure" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Reference to a SITUATION providing further information about the LOG ENTRY.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Delay" type="siri:DurationType" />
        </xsd:sequence>
    </xsd:group>
    <xsd:complexType name="LateDatedVehicleJourneyEntries_RelStructure">
        <xsd:annotation>
            <xsd:documentation>A collection of one or more LateDatedVehicleJourneyEntry instances.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="Entry" type="LateDatedVehicleJourneyEntry_Structure" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>LOGGABLE OBJECT designed for recording DATED VEHICLE JOURNEY information.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
